]:[font = section; inactive; preserveAspect; startGroup]Geometric Definitions:[font = input; preserveAspect; startGroup]x1[t]=-l2 Sin[phi2[t]]+l1 Sin[phi1[t]]:[font = output; output; inactive; preserveAspect; endGroup]l1*Sin[phi1[t]] - l2*Sin[phi2[t]];[o]l1 Sin[phi1[t]] - l2 Sin[phi2[t]]:[font = input; preserveAspect; startGroup]y1[t]=l2 Cos[phi2[t]]-l1 Cos[phi1[t]]:[font = output; output; inactive; preserveAspect; endGroup]-(l1*Cos[phi1[t]]) + l2*Cos[phi2[t]];[o]-(l1 Cos[phi1[t]]) + l2 Cos[phi2[t]]:[font = input; preserveAspect; startGroup]x2[t]=l4 Sin[phi2[t]]:[font = output; output; inactive; preserveAspect; endGroup]l4*Sin[phi2[t]];[o]l4 Sin[phi2[t]]:[font = input; preserveAspect; startGroup]y2[t]=-l4 Cos[phi2[t]]:[font = output; output; inactive; preserveAspect; endGroup]-(l4*Cos[phi2[t]]);[o]-(l4 Cos[phi2[t]]):[font = input; preserveAspect; startGroup]x3[t]=l5 Sin[phi2[t]]+l3 Sin[phi3[t]]:[font = output; output; inactive; preserveAspect; endGroup]l5*Sin[phi2[t]] + l3*Sin[phi3[t]];[o]l5 Sin[phi2[t]] + l3 Sin[phi3[t]]:[font = input; preserveAspect; startGroup]y3[t]=-l5 Cos[phi2[t]]-l3 Cos[phi3[t]]:[font = output; output; inactive; preserveAspect; endGroup]-(l5*Cos[phi2[t]]) - l3*Cos[phi3[t]];[o]-(l5 Cos[phi2[t]]) - l3 Cos[phi3[t]]:[font = subsection; inactive; preserveAspect; startGroup]When the projectile is still on the ground:[font = input; preserveAspect; startGroup]phi3[t]==-Part[Solve[-h==y3[t],phi3[t]],1,1,2]:[font = message; inactive; preserveAspect]Solve::ifun: Warning: Inverse functions are being used by Solve, so some     solutions may not be found.:[font = output; output; inactive; preserveAspect; endGroup]phi3[t] == -ArcCos[(h - l5*Cos[phi2[t]])/l3];[o]                   h - l5 Cos[phi2[t]]phi3[t] == -ArcCos[-------------------]                           l3:[font = input; preserveAspect; startGroup]Def1=Part[Solve[%,phi3[t]],1,1]:[font = output; output; inactive; preserveAspect; endGroup]phi3[t] -> -ArcCos[(h - l5*Cos[phi2[t]])/l3];[o]                   h - l5 Cos[phi2[t]]phi3[t] -> -ArcCos[-------------------]                           l3:[font = input; preserveAspect; startGroup]D[Def1,t]:[font = output; output; inactive; preserveAspect; endGroup]Derivative[1][phi3][t] ->    (l5*Sin[phi2[t]]*Derivative[1][phi2][t])/    (l3*(1 - (h - l5*Cos[phi2[t]])^2/l3^2)^(1/2));[o]                 l5 Sin[phi2[t]] phi2'[t]phi3'[t] -> -----------------------------------                                             2                        (h - l5 Cos[phi2[t]])            l3 Sqrt[1 - ----------------------]                                   2                                 l3:[font = input; preserveAspect; startGroup]CForm[Part[%,2]]:[font = output; output; inactive; preserveAspect; fontLeading = 0; endGroup; endGroup; endGroup]l5*Sin(phi2(t))*Derivative(1)(phi2)(t)/   (l3*Sqrt(1 - Power(h - l5*Cos(phi2(t)),2)/Power(l3,2)));[o]l5*Sin(phi2(t))*Derivative(1)(phi2)(t)/   (l3*Sqrt(1 - Power(h - l5*Cos(phi2(t)),2)/Power(l3,2))):[font = section; inactive; preserveAspect; startGroup]Equations of Motion:[font = input; preserveAspect; startGroup]T=Simplify[1/2 m1 (D[x1[t],t]^2+D[y1[t],t]^2)          +1/2 J  (D[phi2[t],t]^2)          +1/2 m2 (D[x2[t],t]^2+D[y2[t],t]^2)*0          +1/2 m3 (D[x3[t],t]^2+D[y3[t],t]^2)]:[font = output; output; inactive; preserveAspect; endGroup](l1^2*m1*Derivative[1][phi1][t]^2 -      2*l1*l2*m1*Cos[phi1[t] - phi2[t]]*Derivative[1][phi1][t]*      Derivative[1][phi2][t] + J*Derivative[1][phi2][t]^2 +      l2^2*m1*Derivative[1][phi2][t]^2 + l5^2*m3*Derivative[1][phi2][t]^2 +      2*l3*l5*m3*Cos[phi2[t] - phi3[t]]*Derivative[1][phi2][t]*      Derivative[1][phi3][t] + l3^2*m3*Derivative[1][phi3][t]^2)/2;[o]   2            2(l1  m1 phi1'[t]  - 2 l1 l2 m1 Cos[phi1[t] - phi2[t]] phi1'[t] phi2'[t] +                2     2            2     2            2    J phi2'[t]  + l2  m1 phi2'[t]  + l5  m3 phi2'[t]  +                                                              2            2    2 l3 l5 m3 Cos[phi2[t] - phi3[t]] phi2'[t] phi3'[t] + l3  m3 phi3'[t] )     / 2:[font = input; preserveAspect; startGroup]U=Simplify[m1 g y1[t]+m2 g y2[t]+m3 g y3[t]]:[font = output; output; inactive; preserveAspect; endGroup]g*(-(l1*m1*Cos[phi1[t]]) + l2*m1*Cos[phi2[t]] - l4*m2*Cos[phi2[t]] -      l5*m3*Cos[phi2[t]] - l3*m3*Cos[phi3[t]]);[o]g (-(l1 m1 Cos[phi1[t]]) + l2 m1 Cos[phi2[t]] - l4 m2 Cos[phi2[t]] -      l5 m3 Cos[phi2[t]] - l3 m3 Cos[phi3[t]]):[font = input; preserveAspect; startGroup]D[phi1[t],t]:[font = output; output; inactive; preserveAspect; endGroup]Derivative[1][phi1][t];[o]phi1'[t]:[font = input; preserveAspect; startGroup]Eqn1=Simplify[D[D[T,D[phi1[t],t]],t]-D[T,phi1[t]]+D[U,phi1[t]]==0]:[font = output; output; inactive; preserveAspect; endGroup]l1*m1*(g*Sin[phi1[t]] - l2*Sin[phi1[t] - phi2[t]]*       Derivative[1][phi2][t]^2 + l1*Derivative[2][phi1][t] -       l2*Cos[phi1[t] - phi2[t]]*Derivative[2][phi2][t]) == 0;[o]                                                          2l1 m1 (g Sin[phi1[t]] - l2 Sin[phi1[t] - phi2[t]] phi2'[t]  +       l1 phi1''[t] - l2 Cos[phi1[t] - phi2[t]] phi2''[t]) == 0:[font = input; preserveAspect; startGroup]Eqn2=Simplify[D[D[T,D[phi2[t],t]],t]-D[T,phi2[t]]+D[U,phi2[t]]==0]:[font = output; output; inactive; preserveAspect; endGroup]-(g*l2*m1*Sin[phi2[t]]) + g*l4*m2*Sin[phi2[t]] + g*l5*m3*Sin[phi2[t]] +     l1*l2*m1*Sin[phi1[t] - phi2[t]]*Derivative[1][phi1][t]^2 +     l3*l5*m3*Sin[phi2[t] - phi3[t]]*Derivative[1][phi3][t]^2 -     l1*l2*m1*Cos[phi1[t] - phi2[t]]*Derivative[2][phi1][t] +     J*Derivative[2][phi2][t] + l2^2*m1*Derivative[2][phi2][t] +     l5^2*m3*Derivative[2][phi2][t] +     l3*l5*m3*Cos[phi2[t] - phi3[t]]*Derivative[2][phi3][t] == 0;[o]-(g l2 m1 Sin[phi2[t]]) + g l4 m2 Sin[phi2[t]] + g l5 m3 Sin[phi2[t]] +                                             2   l1 l2 m1 Sin[