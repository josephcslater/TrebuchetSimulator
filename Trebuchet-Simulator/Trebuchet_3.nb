                     2l1 m1 (g Sin[phi1[t]] - l2 Sin[phi1[t] - phi2[t]] phi2'[t]  +       l1 phi1''[t] - l2 Cos[phi1[t] - phi2[t]] phi2''[t]) == 0\\>"], "Output",\  ImageRegion->{{0, 1}, {0, 1}}]}, Open]],Cell[CellGroupData[{Cell["\<\Eqn2=Simplify[D[D[T,D[phi2[t],t]],t]-D[T,phi2[t]]+D[U,phi2[t]]==0]\\\>", "Input",  ImageRegion->{{0, 1}, {0, 1}}],Cell[OutputFormData["\<\-(g*l2*m1*Sin[phi2[t]]) + g*l4*m2*Sin[phi2[t]] + \g*l5*m3*Sin[phi2[t]] +     l1*l2*m1*Sin[phi1[t] - phi2[t]]*Derivative[1][phi1][t]^2 +     l3*l5*m3*Sin[phi2[t] - phi3[t]]*Derivative[1][phi3][t]^2 -     l1*l2*m1*Cos[phi1[t] - phi2[t]]*Derivative[2][phi1][t] +     J*Derivative[2][phi2][t] + l2^2*m1*Derivative[2][phi2][t] +     l5^2*m3*Derivative[2][phi2][t] +     l3*l5*m3*Cos[phi2[t] - phi3[t]]*Derivative[2][phi3][t] == 0\\>", "\<\-(g \l2 m1 Sin[phi2[t]]) + g l4 m2 Sin[phi2[t]] + g l5 m3 Sin[phi2[t]] +                                             2   l1 l2 m1 Sin[phi1[t] - phi2[t]] phi1'[t]  +                                             2   l3 l5 m3 Sin[phi2[t] - phi3[t]] phi3'[t]  -     l1 l2 m1 Cos[phi1[t] - phi2[t]] phi1''[t] + J phi2''[t] +       2                  2   l2  m1 phi2''[t] + l5  m3 phi2''[t] +     l3 l5 m3 Cos[phi2[t] - phi3[t]] phi3''[t] == 0\\>"], "Output",  ImageRegion->{{0, 1}, {0, 1}}]}, Open]],Cell[CellGroupData[{Cell["\<\Eqn3=Simplify[D[D[T,D[phi3[t],t]],t]-D[T,phi3[t]]+D[U,phi3[t]]==0]\\\>", "Input",  ImageRegion->{{0, 1}, {0, 1}}],Cell[OutputFormData["\<\l3*m3*(g*Sin[phi3[t]] - l5*Sin[phi2[t] - phi3[t]]*       Derivative[1][phi2][t]^2 +       l5*Cos[phi2[t] - phi3[t]]*Derivative[2][phi2][t] +       l3*Derivative[2][phi3][t]) == 0\\>", "\<\                              \                            2l3 m3 (g Sin[phi3[t]] - l5 Sin[phi2[t] - phi3[t]] phi2'[t]  +       l5 Cos[phi2[t] - phi3[t]] phi2''[t] + l3 phi3''[t]) == 0\\>"], "Output",\  ImageRegion->{{0, 1}, {0, 1}}]}, Open]],Cell["\<\StateEquations=Solve